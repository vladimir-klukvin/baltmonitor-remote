{
    "[c]": {
        "editor.rulers": [
            80
        ],
        "files.insertFinalNewline": true,
        "editor.glyphMargin": true,
    },
    "files.associations": {
        "*.C": "c",
        "*.c": "c",
        "*.H": "c",
        "*.h": "c",
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "baltmonitor-remote": true
    }
}